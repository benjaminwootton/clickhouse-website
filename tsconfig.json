{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "styles/*": ["src/styles/*"],
      "assets/*": ["src/assets/*"],
      "layouts/*": ["src/layouts/*"],
    }
  }
}